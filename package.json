{
  "name": "fox-qppi",
  "version": "1.0.0",
  "description": "Fox QPPI: QR-DNA security code access with KMS integration",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "tsc",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:coverage": "node --experimental-vm-modules node_modules/jest/bin/jest.js --coverage",
    "lint": "eslint src tests --ext .ts",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "stress": "node dist/tools/stress.js",
    "keyset": "node dist/http/keyset.js",
    "rotate-key": "node dist/scripts/rotate-kms-key.js",
    "revoke-key": "node dist/scripts/revoke-key.js",
    "stress:dev": "node --import ts-node/esm src/tools/stress.ts",
    "keyset:dev": "node --import ts-node/esm src/http/keyset.ts",
    "rotate-key:dev": "node --import ts-node/esm scripts/rotate-kms-key.ts",
    "revoke-key:dev": "node --import ts-node/esm scripts/revoke-key.ts"
  },
  "keywords": [
    "qr-dna",
    "security",
    "kms",
    "ed25519",
    "authentication"
  ],
  "author": "Space-LEAF-corp",
  "license": "Apache-2.0",
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.450.0",
    "@aws-sdk/client-kms": "^3.450.0",
    "@aws-sdk/client-s3": "^3.450.0",
    "@aws-sdk/lib-dynamodb": "^3.450.0",
    "ajv": "^8.12.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "tweetnacl": "^1.0.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2"
  }
}
