# AWS Configuration
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=YOUR_ACCESS_KEY
AWS_SECRET_ACCESS_KEY=YOUR_SECRET_KEY

# KMS Configuration
KMS_KEY_ID=YOUR_KMS_KEY_ID
KMS_KEY_ARN=YOUR_KMS_KEY_ARN

# DynamoDB Configuration
DYNAMODB_TABLE_NAME=fox-qppi-key-registry
DYNAMODB_ENDPOINT=http://localhost:8000

# Key Registry Configuration
KEY_REGISTRY_TYPE=file
KEY_REGISTRY_PATH=./data/keys.json

# HTTP Server Configuration
KEYSET_PORT=3000
KEYSET_HOST=localhost

# Monitoring and Alerts
WEBHOOK_URL=https://example.com/webhook
WEBHOOK_SECRET=YOUR_WEBHOOK_SECRET
ENABLE_MONITORING=true
ALERT_THRESHOLD=100

# Stress Testing
STRESS_DURATION_MS=60000
STRESS_CONCURRENCY=10
STRESS_S3_BUCKET=fox-qppi-stress-results

# Development
NODE_ENV=development
LOG_LEVEL=info
